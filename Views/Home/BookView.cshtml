@{
    ViewData["Title"] = "BookView";
}

@model BooksViewModel

<div>

    @*create a for loop to print each row in our table*@
    @foreach (Book b in Model.Books)
    {
        <partial name="BookSummary" model="b"/>
    }

</div>

<div page-blah="@Model.PageInfo" 
     page-action="BookView" 
     page-class="btn"
     page-class-normal='btn-outline-dark' 
     page-class-selected='btn-primary'
     page-classes-enabled="true"
     class="btn-group pull-right m-1">
</div>
